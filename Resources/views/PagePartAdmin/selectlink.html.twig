      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <!-- Le styles -->
    {% stylesheets
    	'bundles/kunstmaanadmin/css/bootstrap.css'
    	'bundles/kunstmaanadmin/css/style.css'
    	'bundles/kunstmaanadmin/js/libs/themes/OMNext/style.css'
        output='generated-css/style.css'
        filter='cssrewrite'
    %}
    <link rel="stylesheet"  media="screen" type="text/css" href="{{ asset_url }}" />
    {% endstylesheets %}
    {#
    {% javascripts
    	'@KunstmaanAdminBundle/Resources/public/js/libs/modernizr-2.0.6.min.js'
    	'@KunstmaanAdminBundle/Resources/public/js/libs/jquery-1.7.1.min.js'
    	'@KunstmaanAdminBundle/Resources/public/js/libs/jquery-ui-1.8.16.custom.min.js'
    	'@KunstmaanAdminBundle/Resources/public/js/libs/bootstrap-twipsy.js'
        '@KunstmaanAdminBundle/Resources/public/js/libs/*'
        '@KunstmaanAdminBundle/Resources/public/js/ckeditor/ckeditor.js'
    	'@KunstmaanAdminBundle/Resources/public/js/*'
    	output='generated-js/scripts.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    #}
    <script src="{{ asset('bundles/kunstmaanadmin/js/libs/modernizr-2.0.6.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/kunstmaanadmin/js/libs/jquery-1.7.1.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/kunstmaanadmin/js/libs/jquery-ui-1.8.16.custom.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/kunstmaanadmin/js/libs/jquery.jstree.js') }}" type="text/javascript"></script>
 	<script src="{{ asset('bundles/kunstmaanadmin/js/script.js') }}" type="text/javascript"></script>
 	<!-- Le fav and touch icons -->
    <link rel="shortcut icon" href="{{ asset('bundles/kunstmaanadmin/img/favicon.ico') }}">
    <link rel="apple-touch-icon" href="{{ asset('bundles/kunstmaanadmin/img/apple-touch-icon.png') }}">
    <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('bundles/kunstmaanadmin/img/apple-touch-icon-72x72.png') }}">
    <link rel="apple-touch-icon" sizes="114x114" href="{{ asset('bundles/kunstmaanadmin/img/apple-touch-icon-114x114.png') }}">
    <script type="text/javascript">
		// Close the dialog
	    function closeme() {
	    	window.close()
	    }

	    // Handle click of OK button
	    function handleOK(result) {
	    	if (window.opener) {
	    	    window.opener.dialogWin.returnedValue = result;
	    		window.opener.dialogWin.returnFunc()
	    	} else {
	    		//alert("You have closed the main window.\n\nNo action will be taken on the choices in this dialog box.")
	    	}
	    	window.closeme()
	    	return false
	    }

	    // Handle click of Cancel button
	    function handleCancel() {
	    	window.closeme()
	    	return false
	    }

	    function doBodyLoad() {
	    	if (window.opener) {
	    		if (window.opener.blockEvents) {
	    			window.opener.blockEvents();
	    		}
	    	}
	    }

	    function doBodyUnload() {
	    	if (window.opener) {
	    		if (window.opener.unblockEvents) {
	    			window.opener.unblockEvents();
	    		}
	    	}
	    }
    </script>
  </head>
  <body>
<div id="tree">
        <ul>
            {% for node in nodemenu.topnodes %}
                {% include 'KunstmaanPagePartBundle:PagePartAdmin:rectreeview.html.twig' %}
            {% endfor %}
        </ul>
    </div>   
</body>
</html>